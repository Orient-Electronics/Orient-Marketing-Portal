<%- model_class = Shop -%>
<div class="page-header">
  <h1><%= "Searching Results - " %> <%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<div>
  <ul>
    <% @shops.each do |shop| %>
    <li><%= shop.dealer_name%></li>
    
    <% end %>
    <%= will_paginate @shops%>
  </ul>
  
</div>
